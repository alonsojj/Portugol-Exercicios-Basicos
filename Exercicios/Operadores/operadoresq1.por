programa
{
	funcao inicio () //Tem a função de identificar se o númeroémaior que 10 e se for subtrai 5//
	{
		inteiro numero1, numero2//define as variáveis para numeros//
		escreva("Escreva um número:\n")
		leia(numero1)//lê o numero como variável numero1 e permite a escrita do número que quer//
		
		se(numero1>10)//se o número for maior que 10, Subtrai 5//
		{
			escreva(numero1-5)
			leia(numero2)
		}
	}
}
